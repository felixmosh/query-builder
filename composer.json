{
  "name": "felixmosh/query-builder",
  "type": "library",
  "description": "A slim query-builder using php mysqli extension",
  "keywords": ["msqli","query-builder"],
  "homepage": "https://www.github.com/felixmosh/query-builder",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "felixmosh"
    }
  ],
  "require": {
    "php": ">5.6",
    "ext-mysqli": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0"
  },
  "autoload": {
    "psr-4": {
      "QueryBuilder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QueryBuilder\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "@phpunit"
    ],
    "phpunit": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}